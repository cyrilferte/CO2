 <%= simple_form_for @contenue do |f| %>
      <%= f.input :title, placeholder: "Name", label: "Title", class: "text-left" %>
      <%= f.input :abstract, placeholder: "Short resume", label: "abstract" %>
      <%= f.input :url, as: :url, label: "link", input_html: { value: "http://" } %>
      <%= f.button :submit, "Add", class: "btn btn-primary" %>
  <% end %>