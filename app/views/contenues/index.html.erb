<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1 class="text-center"> All products </h1>
      <% @contenues.each do |contenue| %>
      <div class="product">

 <%= link_to contenue.url do %>

         <div class='product-body'>
            <h3 class="ttext"><%= contenue.title %></h3>
            <p class="ttext"><%= contenue.abstract %></p>
           <% end %> 
          </div>

        </div>
      <% end %>
    </div>
  </div>
</div>